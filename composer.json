{
    "name": "alexusmai/kafka-log-driver",
    "description": "Laravel, log driver for Kafka",
    "keywords": [
        "alexusmai",
        "laravel",
        "kafka",
        "log",
        "driver"
    ],
    "homepage": "https://github.com/alexusmai/kafka-log-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Manekin",
            "email": "alexusmai@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-rdkafka": "^6.0",
        "illuminate/support": "^9.0",
        "illuminate/contracts": "^9.0",
        "monolog/monolog": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Alexusmai\\KafkaLogDriver\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alexusmai\\KafkaLogDriver\\KafkaLogDriverServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
